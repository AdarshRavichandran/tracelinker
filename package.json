{
  "name": "@tracelinker/action",
  "version": "0.1.0",
  "description": "GitHub Action for automatic issue-PR-file-test traceability",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.js -o dist",
    "test": "jest",
    "test:action": "node dist/index.js",
    "lint": "eslint src --fix",
    "format": "prettier --write 'src/**/*.js'",
    "dev": "node src/index.js"
  },
  "keywords": [
    "github",
    "action",
    "traceability",
    "issue-tracking",
    "pr-automation",
    "devops"
  ],
  "author": "TraceLinker Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tracelinker-org/tracelinker.git"
  },
  "bugs": {
    "url": "https://github.com/tracelinker-org/tracelinker/issues"
  },
  "homepage": "https://github.com/tracelinker-org/tracelinker#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0",
    "js-yaml": "^4.1.0",
    "minimatch": "^9.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

